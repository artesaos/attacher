{
  "name": "artesaos/attacher",
  "description": "Annex and do what you want with your pictures! For Laravel 5",
  "license": "MIT",
  "keywords" : ["laravel5", "laravel", "flysystem", "upload", "AwsS3", "Azure", "Copy", "Dropbox", "GridFS", "Rackspace", "Sftp", "WebDav", "ZipAdapter"],
  "autoload": {
    "psr-4": {
      "Artesaos\\Attacher\\": "src/"
    }
  },
  "require": {
    "illuminate/support": "~5",
    "intervention/image": "~2.3",
    "graham-campbell/flysystem": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload-dev": {
    "psr-4": {
      "Vinicius73\\Attacher\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Artesaos\\Attacher\\Providers\\AttacherServiceProvider"
      ],
      "aliases": [
        "Attacher": "Artesaos\\Attacher\\Facades\\Attacher"
      ]
    }
  }
}
